<IfModule mod_rewrite.c>
   RewriteEngine On
   # Force SSL
   RewriteCond %{HTTP_HOST} ^https://scheduling.bookingkh.com/ [NC]
   RewriteCond %{SERVER_PORT} 80
   RewriteRule ^(.*)$ https://https://scheduling.bookingkh.com//$1 [R,L]
   # Remove public folder form URL
   RewriteRule ^(.*)$ public/$1 [L]
</IfModule>